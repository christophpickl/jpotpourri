<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%">
	
	<mx:Script>
		<![CDATA[
			import net.sourceforge.jpotpourri.learnme.model.ModelLocator;
			
			private const model: ModelLocator = ModelLocator.instance;
			
		]]>
	</mx:Script>
	
	<mx:Label text="finished" />
	
	<mx:DataGrid dataProvider="{model.questionairy.checkedQuestions}">
		<mx:columns>
			<mx:DataGridColumn dataField="title" headerText="Title" />
			<mx:DataGridColumn dataField="checkedAllCorrect" headerText="Correct" />
		</mx:columns>
	</mx:DataGrid>
	
	<mx:Button label="Back Home" click="parentApplication.currentState=''" />
	
</mx:VBox>
